<!-- db/changelog-1.1-container.xml -->

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

  <!-- Create Container Table -->
    <changeSet id="1" author="your-name">
    <createTable tableName="proprietaire">
      <column name="id" type="BIGINT">
        <constraints primaryKey="true" nullable="false"/>
      </column>

        <column name="personne_id" type="BIGINT">
          <constraints nullable="false"/>
        </column>

    </createTable>
  </changeSet>


    <!-- Create Sequence for ContainerType -->
  <changeSet id="2" author="your-name">
    <createSequence sequenceName="proprietaire_seq" startValue="1" incrementBy="1"/>
  </changeSet>

  <!-- Load Data from CSV -->
  <changeSet id="3" author="your-name">
    <loadData
            file="classpath:/db/migration/data/gestionchequeffet/Proprietaire.csv"
            tableName="proprietaire">
      <column name="id" type="NUMERIC"/>
        <column name="personne" type="BIGINT"/>
    </loadData>
  </changeSet>


</databaseChangeLog>
